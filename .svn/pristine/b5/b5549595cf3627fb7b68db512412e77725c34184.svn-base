    <script>
        const eleBanSwiper = new Swiper(".main-data-banner .swiper", {
          slidesPerView: 1,
          spaceBetween: 16,
          speed: 400,
          loop: true,
          autoplay: {
            delay: 2500,
            disableOnInteraction: false,
            pauseOnMouseEnter: true,
          },
          navigation: {
            nextEl: ".main-data-banner .swiper-button-next",
            prevEl: ".main-data-banner .swiper-button-prev",
          },
          pagination: {
            el: ".main-data-banner .swiper-pagination",
            type: "fraction",
          },
        });
        const $eleBanSwiperPlay = document.querySelector(".main-data-banner .swiper-button-play");
        const $eleBanSwiperStop = document.querySelector(".main-data-banner .swiper-button-stop");
        $eleBanSwiperPlay.style.display = "none";
        $eleBanSwiperPlay.addEventListener("click", () => {
          eleBanSwiper.autoplay.start();
          $eleBanSwiperStop.style.display = "";
          $eleBanSwiperPlay.style.display = "none";
        });
        $eleBanSwiperStop.addEventListener("click", () => {
          eleBanSwiper.autoplay.stop();
          $eleBanSwiperStop.style.display = "none";
          $eleBanSwiperPlay.style.display = "";
        });
    </script>
